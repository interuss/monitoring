{
  "$id": "https://github.com/interuss/monitoring/blob/main/schemas/monitoring/uss_qualifier/resources/netrid/flight_data_resources/FlightDataStorageSpecification.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "monitoring.uss_qualifier.resources.netrid.flight_data_resources.FlightDataStorageSpecification, as defined in monitoring/uss_qualifier/resources/netrid/flight_data_resources.py",
  "properties": {
    "$ref": {
      "description": "Path to content that replaces the $ref",
      "type": "string"
    },
    "flight_record_collection_path": {
